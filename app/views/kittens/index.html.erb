<h1>All kittens</h1>

<% @kittens.each do |kitten| %>
  <p>Name: <%= kitten.name %></p>
  <p>Age:<%= kitten.age %></p>
  <p>Softness:<%= kitten.softness %></p>
  <p>Cuteness:<%= kitten.cuteness %></p>
  <%= button_to "Edit kitten", edit_kitten_path(kitten), method: :get%>
<% end %>

<%= button_to "Create a new kitten", new_kitten_path, method: :get %>